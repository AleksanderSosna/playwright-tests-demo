{
    "compilerOptions": {
        "baseUrl": ".",
        "esModuleInterop": true,
        "lib": [
            "dom",
            "dom.iterable",
            "es2019"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "noEmit": true,
        "paths": {
            "@fixtures/*": [
                "fixtures/*"
            ],
            "@page-objects/*": [
                "page-objects/*"
            ],
            "@tests/*": [
                "tests/*"
            ]
        },
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": false,
        "target": "es2019",
        "types": [
            "node",
        ]
    },
    "exclude": [],
    "includes": []
}